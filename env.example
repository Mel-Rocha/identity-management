# DJANGO CONFIGS
DEBUG="<debug_mode:bool>"
ALLOWED_HOSTS="*"
SECRET_KEY="<secret_key:str>"
CORS_ALLOWED_ORIGINS=http://<ip_address>,http://<ip_address>:<port>,http://<ip_address>:<port>
CSRF_TRUSTED_ORIGINS=http://<domain>,https://<domain>,http://<ip_address>:<port>,http://<ip_address>:<port>

# DATABASE CONFIG
DATABASE_URL="postgresql://<db_user>:<db_password>@<db_host>:<db_port>/<db_name>"

# REDIS CONFIG
REDIS_HOST=<redis_host:str>
REDIS_PORT=<redis_port:int>
REDIS_DB=<redis_db:int>

# EMAIL CONFIG
EMAIL_HOST=<email_host:str>
EMAIL_PORT=<email_port:int>
EMAIL_USE_TLS=<email_use_tls:bool>
EMAIL_HOST_USER=<email_host_user:str>
EMAIL_HOST_PASSWORD=<email_host_password:str>
DEFAULT_FROM_EMAIL=<default_from_email:str>
